{
  "name": "cookapp",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "nest build",
    "start:prod": "node --optimize_for_size --max_old_space_size=460 --gc_interval=100 dist/main.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --watch",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "copy:assets": "cpx 'src/templates/**' 'dist/templates'",
    "migration:create": "ts-node node_modules/typeorm/cli.js migration:create -f ormconfig -d src/migrations -n",
    "migration:generate": "ts-node node_modules/typeorm/cli.js migration:generate -f ormconfig -d src/migrations -n",
    "migration:revert": "ts-node node_modules/typeorm/cli.js migration:revert -f ormconfig",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js -f ormconfig migration:run"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.3.17",
    "@nestjs/axios": "^0.0.4",
    "@nestjs/bull": "^0.4.2",
    "@nestjs/cli": "^8.1.6",
    "@nestjs/common": "^8.2.4",
    "@nestjs/core": "^8.2.4",
    "@nestjs/cqrs": "^8.0.1",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.2.4",
    "@nestjs/platform-socket.io": "^8.4.0",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/typeorm": "^8.0.3",
    "@nestjs/websockets": "^8.4.0",
    "@sentry/node": "^6.15.0",
    "@types/multer": "^1.4.7",
    "bcrypt": "^5.0.0",
    "build": "^0.1.4",
    "bull": "^3.29.3",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^7.0.0",
    "express-session": "^1.17.2",
    "firebase-admin": "^10.0.0",
    "google-auth-library": "^7.10.2",
    "handlebars": "^4.7.7",
    "helmet": "^3.21.2",
    "lodash": "^4.17.20",
    "morgan": "^1.10.0",
    "multer-s3": "^2.10.0",
    "nanoid": "^3.1.30",
    "nest-raven": "^8.0.0",
    "nestjs-config": "^1.4.10",
    "nodemailer": "^6.7.2",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-oauth": "^1.0.0",
    "pg": "^8.7.3",
    "random-words": "^1.1.0",
    "reflect-metadata": "^0.1.13",
    "request-context": "^2.0.0",
    "rxjs": "^7.4.0",
    "snakecase-keys": "^3.1.2",
    "swagger-ui-express": "^4.0.2",
    "typeorm": "^0.2.45"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.1",
    "@types/bull": "^3.15.6",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/lodash": "^4.14.177",
    "@types/morgan": "^1.9.3",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^14.17.34",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-local": "^1.0.34",
    "nodemon": "^2.0.15",
    "ts-node": "^9.1.1",
    "typescript": "^4.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": "14.17.6",
    "npm": "8.3.0"
  }
}
